@prefix : <https://voc.europe-geology.eu/ont/eugm#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <https://voc.europe-geology.eu/ont/eugm#> .

<https://voc.europe-geology.eu/ont/eugm#> rdf:type owl:Ontology ;
                                           dc:creator "Sőrés László, SARA" ;
                                           dc:description "Simplified ontology for EUGM, 2025.04.25" ;
                                           dc:title "Ontology for the European Groundwater Monitoring Database (EUGM)" ;
                                           rdfs:label "EUGM" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/conformsTo
dct:conformsTo rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/identifier
dct:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#label
rdf:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/depiction
foaf:depiction rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://voc.europe-geology.eu/ont/eugm#aquiferCodelistProperty
:aquiferCodelistProperty rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty .


###  https://voc.europe-geology.eu/ont/eugm#aquiferType
:aquiferType rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :aquiferCodelistProperty ;
             rdfs:domain :Aquifer ;
             rdfs:range <https://inspire.ec.europa.eu/codelist/AquiferTypeValue> ;
             skos:altLabel "aquiferType" .


###  https://voc.europe-geology.eu/ont/eugm#datastreams
:datastreams rdf:type owl:ObjectProperty ;
             owl:inverseOf :thing ;
             rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#thing> ;
             rdfs:range <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#datastream> .


###  https://voc.europe-geology.eu/ont/eugm#deployment
:deployment rdf:type owl:ObjectProperty ;
            rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#sensor> ;
            rdfs:range :Deployment .


###  https://voc.europe-geology.eu/ont/eugm#deviceType
:deviceType rdf:type owl:ObjectProperty ;
            rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#sensor> ;
            rdfs:range :DeviceTypeValue ;
            skos:altLabel "measuringType" .


###  https://voc.europe-geology.eu/ont/eugm#forecastingCodelistProperty
:forecastingCodelistProperty rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty .


###  https://voc.europe-geology.eu/ont/eugm#forecastingMethod
:forecastingMethod rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :forecastingCodelistProperty ;
                   rdfs:domain :ForecastedTimeseries ;
                   rdfs:range :ForecastingMethodValue ;
                   skos:altLabel "methodForecast" .


###  https://voc.europe-geology.eu/ont/eugm#host
:host rdf:type owl:ObjectProperty ;
      rdfs:domain :Deployment ;
      rdfs:range <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#thing> .


###  https://voc.europe-geology.eu/ont/eugm#imputationMethod
:imputationMethod rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :forecastingCodelistProperty ;
                  rdfs:domain :ForecastedTimeseries ;
                  rdfs:range :FillingMethodValue ;
                  skos:altLabel "methodFill" .


###  https://voc.europe-geology.eu/ont/eugm#measurementType
:measurementType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :monitoringPointCodelistProperty ;
                 rdfs:domain :MonitoringPoint ;
                 rdfs:range :MpMeasurementTypeValue ;
                 skos:altLabel "mpTypeMeasurements" .


###  https://voc.europe-geology.eu/ont/eugm#mediaType
:mediaType rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :aquiferCodelistProperty ;
           rdfs:domain :Aquifer ;
           rdfs:range <https://inspire.ec.europa.eu/codelist/AquiferMediaTypeValue> ;
           skos:altLabel "aquiferMediaType" .


###  https://voc.europe-geology.eu/ont/eugm#meteoDataType
:meteoDataType rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :forecastingCodelistProperty ;
               rdfs:domain :ForecastedTimeseries ;
               rdfs:range :MeteoDataTypeValue ;
               skos:altLabel "meteoData" .


###  https://voc.europe-geology.eu/ont/eugm#monitoringPointCodelistProperty
:monitoringPointCodelistProperty rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf owl:topObjectProperty .


###  https://voc.europe-geology.eu/ont/eugm#monitoringPointType
:monitoringPointType rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :monitoringPointCodelistProperty ;
                     rdfs:domain :MonitoringPoint ;
                     rdfs:range :MonitoringPointTypeValue ;
                     skos:altLabel "mpType" .


###  https://voc.europe-geology.eu/ont/eugm#observations
:observations rdf:type owl:ObjectProperty ;
              rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#datastream> ;
              rdfs:range <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#observation> .


###  https://voc.europe-geology.eu/ont/eugm#observedProperty
:observedProperty rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#datastream> ;
                  rdfs:range <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#observedproperty> .


###  https://voc.europe-geology.eu/ont/eugm#observingProcedure
:observingProcedure rdf:type owl:ObjectProperty ;
                    rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#datastream> ;
                    rdfs:range :ObservingProcedure .


###  https://voc.europe-geology.eu/ont/eugm#optimizationMethod
:optimizationMethod rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :forecastingCodelistProperty ;
                    rdfs:domain :ForecastedTimeseries ;
                    rdfs:range :OptimizationMethodValue ;
                    skos:altLabel "optimization" .


###  https://voc.europe-geology.eu/ont/eugm#pumpingEffect
:pumpingEffect rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :monitoringPointCodelistProperty ;
               rdfs:domain :MonitoringPoint ;
               rdfs:range :PumpingEffectValue ;
               skos:altLabel "pumping" .


###  https://voc.europe-geology.eu/ont/eugm#sensor
:sensor rdf:type owl:ObjectProperty ;
        rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#datastream> ;
        rdfs:range <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#sensor> .


###  https://voc.europe-geology.eu/ont/eugm#thing
:thing rdf:type owl:ObjectProperty ;
       rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#datastream> ;
       rdfs:range <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#thing> .


###  https://voc.europe-geology.eu/ont/eugm#timeResolution
:timeResolution rdf:type owl:ObjectProperty ;
                rdfs:domain <http://www.opengis.net/spec/iot_sensing/1.1/req/datamodel#sensor> ;
                rdfs:range :TimeResolutionValue ;
                skos:altLabel "timeResolution" .


###  https://voc.europe-geology.eu/ont/eugm#trendAnalysisCodelistProperty
:trendAnalysisCodelistProperty rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf owl:topObjectProperty .


###  https://voc.europe-geology.eu/ont/eugm#trendAnalysisMethod
:trendAnalysisMethod rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :trendAnalysisCodelistProperty ;
                     rdfs:domain :TrendAnalysis ;
                     rdfs:range :TrendAnalysisMethodValue ;
                     skos:altLabel "tac_method" .


#################################################################
#    Data properties
#################################################################

###  https://voc.europe-geology.eu/ont/eugm#accumulationPeriod
:accumulationPeriod rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :standardizationProperty ;
                    rdfs:domain :StandardizedTimeseries ;
                    rdfs:range xsd:float ;
                    dc:description "Accumulation period associated with stsc_ccmax" ;
                    skos:altLabel "stsc_qmax" .


###  https://voc.europe-geology.eu/ont/eugm#alternativeId
:alternativeId rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :monitoringPointProperty ;
               rdfs:domain :MonitoringPoint ;
               rdfs:range xsd:string ;
               dc:description "name of monitoring point at the source of the information" ;
               skos:altLabel "relatedParty_idMP" .


###  https://voc.europe-geology.eu/ont/eugm#aquiferName
:aquiferName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :aquiferProperty ;
             rdfs:domain :Aquifer ;
             rdfs:range xsd:string ;
             dc:description "name of the hidrogeological unit or aquifer to which the MP is related to" ;
             skos:altLabel "relatedAquifer" .


###  https://voc.europe-geology.eu/ont/eugm#aquiferProperty
:aquiferProperty rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty .


###  https://voc.europe-geology.eu/ont/eugm#atDepth
:atDepth rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :deploymentProperty ;
         rdfs:domain :Deployment ;
         rdfs:range xsd:float ;
         dc:description "Vertical depth of monitoring point in meters (from the surface, so without considering the well head or well-curb)" ;
         skos:altLabel "depth" .


###  https://voc.europe-geology.eu/ont/eugm#autoCorrelation
:autoCorrelation rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :standardizationProperty ;
                 rdfs:domain :StandardizedTimeseries ;
                 rdfs:range xsd:float ;
                 dc:description "Autocorrelation of the TS" ;
                 skos:altLabel "stsc_autocorr" .


###  https://voc.europe-geology.eu/ont/eugm#autocorrelation
:autocorrelation rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :forecastingProperty ;
                 rdfs:domain :ForecastedTimeseries ;
                 rdfs:range xsd:float ;
                 dc:description "Measure of  time series autocorrelation with a lag of 9 moths." ;
                 skos:altLabel "autocorrelation" .


###  https://voc.europe-geology.eu/ont/eugm#boreholeProperty
:boreholeProperty rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty .


###  https://voc.europe-geology.eu/ont/eugm#classificationProperty
:classificationProperty rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty .


###  https://voc.europe-geology.eu/ont/eugm#clusterCategory
:clusterCategory rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :forecastingProperty ;
                 rdfs:domain :ForecastedTimeseries ;
                 rdfs:range xsd:integer ;
                 dc:description "Categorical value of each time series refering to the clustering based on defined method." ;
                 skos:altLabel "cluster_cat" .


###  https://voc.europe-geology.eu/ont/eugm#crossCorrelationMaximum
:crossCorrelationMaximum rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :standardizationProperty ;
                         rdfs:domain :StandardizedTimeseries ;
                         rdfs:range xsd:float ;
                         dc:description "Value of maximum cross-correlation with SPI TS " ;
                         skos:altLabel "stsc_ccmax" .


###  https://voc.europe-geology.eu/ont/eugm#deploymentProperty
:deploymentProperty rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty .


###  https://voc.europe-geology.eu/ont/eugm#deploymentTime
:deploymentTime rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :deploymentProperty ;
                rdfs:domain :Deployment ;
                rdfs:range xsd:date ;
                dc:description "Year of the monitoring point con-struction in yyyy format" ;
                skos:altLabel "constructionDate" .


###  https://voc.europe-geology.eu/ont/eugm#distanceToRiver
:distanceToRiver rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :forecastingProperty ;
                 rdfs:domain :ForecastedTimeseries ;
                 rdfs:range xsd:float ;
                 dc:description "Distance of the monitoring point to the closest river." ;
                 skos:altLabel "dist_river" .


###  https://voc.europe-geology.eu/ont/eugm#droughtDurationMaximum
:droughtDurationMaximum rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :standardizationProperty ;
                        rdfs:domain :StandardizedTimeseries ;
                        rdfs:range xsd:float ;
                        dc:description "Maximum duration of drought episodes (in months)" ;
                        skos:altLabel "drought_max_dur" .


###  https://voc.europe-geology.eu/ont/eugm#droughtDurationMean
:droughtDurationMean rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :standardizationProperty ;
                     rdfs:domain :StandardizedTimeseries ;
                     rdfs:range xsd:float ;
                     dc:description "Average duration of all drought episode (in months)" ;
                     skos:altLabel "drought_mean_dur" .


###  https://voc.europe-geology.eu/ont/eugm#droughtDurationMedian
:droughtDurationMedian rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :standardizationProperty ;
                       rdfs:domain :StandardizedTimeseries ;
                       rdfs:range xsd:float ;
                       dc:description "Median duration of all episodes (in months)" ;
                       skos:altLabel "drought_med_dur" .


###  https://voc.europe-geology.eu/ont/eugm#droughtDurationMinimum
:droughtDurationMinimum rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :standardizationProperty ;
                        rdfs:domain :StandardizedTimeseries ;
                        rdfs:range xsd:float ;
                        dc:description "Minum duration of drought episode (in months)" ;
                        skos:altLabel "drought_min_dur" .


###  https://voc.europe-geology.eu/ont/eugm#droughtPeriods
:droughtPeriods rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :standardizationProperty ;
                rdfs:domain :StandardizedTimeseries ;
                rdfs:range xsd:float ;
                dc:description "Number of drought periods based on SGI" ;
                skos:altLabel "drought_number" .


###  https://voc.europe-geology.eu/ont/eugm#droughtReliability
:droughtReliability rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :standardizationProperty ;
                    rdfs:domain :StandardizedTimeseries ;
                    rdfs:range xsd:integer ;
                    dc:description "How likely a system is to not fail = no. of satisfactory conditions / total no. of conditions = 1-fail frequency (SGI thresholds)" ;
                    skos:altLabel "drought_reliability" .


###  https://voc.europe-geology.eu/ont/eugm#droughtResilienceCapacity
:droughtResilienceCapacity rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf :standardizationProperty ;
                           rdfs:domain :StandardizedTimeseries ;
                           rdfs:range xsd:float ;
                           dc:description "Capacity to return to satisfactory conditions from failure = no. of times a satisfactory condition follows and unsatisfactory condition / total no. of unsatisfactory conditions " ;
                           skos:altLabel "drought_resilience1" .


###  https://voc.europe-geology.eu/ont/eugm#droughtResilienceRecover
:droughtResilienceRecover rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf :standardizationProperty ;
                          rdfs:domain :StandardizedTimeseries ;
                          rdfs:range xsd:float ;
                          dc:description "how quickly it recovers from failure = 1/Drought_mean_dur" ;
                          skos:altLabel "drought_resilience2" .


###  https://voc.europe-geology.eu/ont/eugm#droughtSustainability
:droughtSustainability rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :standardizationProperty ;
                       rdfs:domain :StandardizedTimeseries ;
                       rdfs:range xsd:float ;
                       dc:description "Drought_sustainability = Drought_Reliability * Drought_Resilience1 * (1-Drought_vulnerablity ) " ;
                       skos:altLabel "drought_sustainability" .


###  https://voc.europe-geology.eu/ont/eugm#droughtVulnerability
:droughtVulnerability rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf :standardizationProperty ;
                      rdfs:domain :StandardizedTimeseries ;
                      rdfs:range xsd:float ;
                      dc:description """Probabilistic measure accounting for the extent and magnitude of an unsatisfactory condition; Sum of is the magnitude (or severity) of the unsatisfactory condition
multiplied by the probability of that magnitude during the span of the timeseries""" ;
                      skos:altLabel "drought_vulnerability" .


###  https://voc.europe-geology.eu/ont/eugm#elevation
:elevation rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :boreholeProperty ;
           rdfs:domain :Borehole ;
           rdfs:range xsd:float ;
           dc:description "Altitude in m.a.s.l. (meters above sea level) of the surface where the monitoring point is located (not at the top of the well head or well-curb understood as the part of the well that is visible above ground). " ;
           skos:altLabel "elevation" .


###  https://voc.europe-geology.eu/ont/eugm#endDate
:endDate rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :trendAnalysisProperty ;
         rdfs:domain :TrendAnalysis ;
         rdfs:range xsd:date ;
         dc:description "end date of the trend analysis period" ;
         skos:altLabel "tac_end" .


###  https://voc.europe-geology.eu/ont/eugm#entropy
:entropy rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :forecastingProperty ;
         rdfs:domain :ForecastedTimeseries ;
         rdfs:range xsd:float ;
         dc:description "Measure of time series sample entropy." ;
         skos:altLabel "entropy" .


###  https://voc.europe-geology.eu/ont/eugm#forecastingProperty
:forecastingProperty rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty .


###  https://voc.europe-geology.eu/ont/eugm#highestDroughtIntensity
:highestDroughtIntensity rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :standardizationProperty ;
                         rdfs:domain :StandardizedTimeseries ;
                         rdfs:range xsd:string ;
                         dc:description "YYYY-MM of highest drought intensity (lowest SGI of the time series)" ;
                         skos:altLabel "drought_intensity" .


###  https://voc.europe-geology.eu/ont/eugm#highestWetIntensity
:highestWetIntensity rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :standardizationProperty ;
                     rdfs:domain :StandardizedTimeseries ;
                     rdfs:range xsd:string ;
                     dc:description "YYYY-MM of highest wet period intensity (hightest SGI of the time series)" ;
                     skos:altLabel "wet_period_intensity" .


###  https://voc.europe-geology.eu/ont/eugm#imputation
:imputation rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :forecastingProperty ;
            rdfs:domain :ForecastedTimeseries ;
            rdfs:range xsd:float ;
            dc:description "Percentage of gap-filled data" ;
            skos:altLabel "gapFill" .


###  https://voc.europe-geology.eu/ont/eugm#imputedValue
:imputedValue rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :forecastingProperty ;
              rdfs:domain :ForecastingObservation ;
              rdfs:range xsd:boolean ;
              dc:description "Indicates whether the value is observed or imputed. TRUE if the value was imputed or FALSE when not" ;
              skos:altLabel "filled_value" .


###  https://voc.europe-geology.eu/ont/eugm#kedallS
:kedallS rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :trendAnalysisProperty ;
         rdfs:domain :TrendAnalysis ;
         rdfs:range xsd:float ;
         dc:description "Kendall’s S statistic" ;
         skos:altLabel "tac_testS" .


###  https://voc.europe-geology.eu/ont/eugm#kendallConfidenceInterval
:kendallConfidenceInterval rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf :trendAnalysisProperty ;
                           rdfs:domain :TrendAnalysis ;
                           rdfs:range xsd:float ;
                           dc:description "Kendall confidence intervall in %" ;
                           skos:altLabel "tac_testCI" .


###  https://voc.europe-geology.eu/ont/eugm#kendallP
:kendallP rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :trendAnalysisProperty ;
          rdfs:domain :TrendAnalysis ;
          rdfs:range xsd:float ;
          dc:description "Kendall p-value" ;
          skos:altLabel "tac_testP" .


###  https://voc.europe-geology.eu/ont/eugm#kendallTau
:kendallTau rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :trendAnalysisProperty ;
            rdfs:domain :TrendAnalysis ;
            rdfs:range xsd:float ;
            dc:description "Kendall's Tau" ;
            skos:altLabel "tac_testTau" .


###  https://voc.europe-geology.eu/ont/eugm#kendallZ
:kendallZ rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :trendAnalysisProperty ;
          rdfs:domain :TrendAnalysis ;
          rdfs:range xsd:float ;
          dc:description "Kendall's Z statistic" ;
          skos:altLabel "tac_testZ" .


###  https://voc.europe-geology.eu/ont/eugm#lagPeriod
:lagPeriod rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :standardizationProperty ;
           rdfs:domain :StandardizedTimeseries ;
           rdfs:range xsd:integer ;
           dc:description "Lag period associated with stsc_ccmax" ;
           skos:altLabel "stsc_lagmax" .


###  https://voc.europe-geology.eu/ont/eugm#landCover
:landCover rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :forecastingProperty ;
           rdfs:domain :ForecastedTimeseries ;
           rdfs:range xsd:string ;
           dc:description "Main land cover category associated with the monitoring point. " ;
           skos:altLabel "land_cover" .


###  https://voc.europe-geology.eu/ont/eugm#levelShift
:levelShift rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :forecastingProperty ;
            rdfs:domain :ForecastingObservation ;
            rdfs:range xsd:boolean ;
            dc:description "Indicates whether at a given time there is a shift on the GWL observations. TRUE if there is a shift on the TS and FALSE when not" ;
            skos:altLabel "level_shift" .


###  https://voc.europe-geology.eu/ont/eugm#longestDroughtYears
:longestDroughtYears rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :standardizationProperty ;
                     rdfs:domain :StandardizedTimeseries ;
                     rdfs:range xsd:float ;
                     dc:description "Year(s) in which the longest drought event occurred" ;
                     skos:altLabel "drought_max_dur_yrs" .


###  https://voc.europe-geology.eu/ont/eugm#longestWetPeriodYears
:longestWetPeriodYears rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :standardizationProperty ;
